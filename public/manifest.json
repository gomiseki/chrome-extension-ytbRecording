{
  "manifest_version": 2,
  "name": "Home_recording",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "host_permissions": [
    "chrome-extension://*"
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Home_recording",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "tabCapture",
    "activeTab",
    "desktopCapture"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ]
    }
  ]
}